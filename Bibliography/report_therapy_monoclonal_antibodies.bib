	keywords = {Antibodies, Monoclonal, Humans, Hybridomas, Immunotoxins, Enzyme-Linked Immunosorbent Assay, Antigens, Cell Count, Cell Survival, Cells, Cultured, Drug Evaluation, Preclinical, Flow Cytometry, Immunoglobulin Variable Region, Receptors, Antigen, T-Cell, alpha-beta, Sensitivity and Specificity, Suspensions, T-Lymphocytes},
}

