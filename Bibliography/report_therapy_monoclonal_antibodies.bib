	keywords = {Humans, Signal Transduction, antibody, Apoptosis, complement, {ERK}, immunoglobulin, integrin, Models, Immunological, neutrophil, Pathogen-Associated Molecular Pattern Molecules, Phagocytes, phagocytosis, Phagocytosis, Phagosomes, Receptors, Complement, Receptors, {IgG}, Receptors, Immunologic, Receptors, Pattern Recognition},
	file = {Texte intégral:C\:\\Users\\ljtal\\Zotero\\storage\\PI7GIB7T\\Uribe-Querol et Rosales - 2020 - Phagocytosis Our Current Understanding of a Unive.pdf:application/pdf},
}

@article{gordon_phagocytosis_2016,
	title = {Phagocytosis: An Immunobiologic Process},
